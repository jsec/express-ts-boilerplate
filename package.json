{
  "name": "express-ts-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jarrod Seccombe <jsec@hey.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "dev": "concurrently -k -p \"[{name}]\" -n \"App,Spec\" -c \"blue.bold,green.bold\" \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "build": "yarn tsoa spec-and-routes && tsc",
    "test": "yarn mocha",
    "knex": "knex --knexfile src/db/knexfile.ts",
    "db:migrate": "yarn knex migrate:latest",
    "db:migrate:make": "yarn knex migrate:make -x ts",
    "db:seed": "yarn knex seed:run",
    "db:seed:make": "yarn knex seed:make -x ts",
    "db:rollback": "yarn knex migrate:rollback",
    "db:rebuild": "yarn db:rollback --all && yarn db:migrate && yarn db:seed"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "chai": "^4.3.4",
    "concurrently": "^7.0.0",
    "cz-adapter-eslint": "0.3.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "lint-staged": ">=10",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "@types/body-parser": "^1.19.2",
    "body-parser": "^1.19.1",
    "consola": "^2.15.3",
    "cors": "^2.8.5",
    "dotenv": "^14.2.0",
    "express": "^4.18.0",
    "helmet": "^5.0.2",
    "http-status-codes": "^2.2.0",
    "knex": "^1.0.1",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "tslib": "^2.3.1",
    "tsoa": "^4.0.0",
    "tsyringe": "^4.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-adapter-eslint"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
